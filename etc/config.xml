<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
	<default>
		<tun2u_order_migration>
			<settings>
				<access_token></access_token>
				<store_code></store_code>
				<order_prefix>TUN2U-</order_prefix>
				<start_date></start_date>
				<increment_id>0</increment_id>
				<order_id>0</order_id>
				<product_sku></product_sku>
				<salable_quantity>0</salable_quantity>
				<debug_mode>0</debug_mode>
			</settings>
		</tun2u_order_migration>
		<payment>
			<tun2u_payments>
				<active>1</active>
				<model>Tun2U\OrderMigration\Model\Payment\Tun2UPayments</model>
				<order_status>pending</order_status>
				<title>Tun2U Payments</title>
				<allowspecific>0</allowspecific>
				<group>Offline</group>
				<sort_order>100</sort_order>
			</tun2u_payments>
		</payment>
		<carriers>
			<tun2u_shipping>
				<active>1</active>
				<title>Tun2U Shipping</title>
				<name>Tun2U Shipping Method</name>
				<shipping_cost>0</shipping_cost>
				<sallowspecific>0</sallowspecific>
				<sort_order>100</sort_order>
				<model>Tun2U\OrderMigration\Model\Shipping\Tun2UShipping</model>
			</tun2u_shipping>
		</carriers>
	</default>
</config>